#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/admin/Desktop/mortale/m-nest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/admin/Desktop/mortale/m-nest/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prettier@3.1.1/node_modules/prettier/bin/prettier.cjs" "$@"
else
  exec node  "$basedir/../../../../../prettier@3.1.1/node_modules/prettier/bin/prettier.cjs" "$@"
fi
