#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/admin/Desktop/mortale/m-nest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/admin/Desktop/mortale/m-nest/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ts-node@10.9.2_7ln3lwozyybesesf4lghtaucbi/node_modules/ts-node/dist/bin-script.js" "$@"
else
  exec node  "$basedir/../../../../../ts-node@10.9.2_7ln3lwozyybesesf4lghtaucbi/node_modules/ts-node/dist/bin-script.js" "$@"
fi
